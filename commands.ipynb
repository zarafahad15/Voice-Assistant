   "source": [
    "from prophet import Prophet\n",
    "\n",
    "df_prophet = pd.DataFrame({\n",
    "    \"ds\": yearly.index,\n",
    "    \"y\": yearly.values\n",
    "})\n",
    "\n",
    "m = Prophet()\n",
    "m.fit(df_prophet)\n",
    "\n",
    "future = m.make_future_dataframe(periods=20, freq='Y')\n",
    "forecast = m.predict(future)\n",
    "\n",
    "fig1 = m.plot(forecast)\n",
    "fig2 = m.plot_components(forecast)"
   ]
  }
